# Pagina de ESTILO

/*Reset*/
* {
 margin: 0;
 padding: 0;
 font-size: 100%;
 border: none;
 outline: none;
 font-weight: 300;
 box-sizing: border-box;
 font-family: "Lato", sans-serif;
 
}

/*Cabecalho*/
 
.header-1 {
 background-color: #808080;
 height: 60px;
 border-bottom: solid;
}
.logo img {
 height: 65px;
 width: 65px;
}
.facebook ul li {
 display: inline-block;
 float: right;
 margin-left: 5%;
}
.facebook img {
 height: 25px;
 width: 25px;
}

/*Menu*/

.menu {
 display: inline-block;
 background-color: #2f4f4f;
 width: 100%;
 border-bottom: solid;
}
.menuplus ul li {
 display: inline-block;
 color: #fff5ee;
 margin-left: 3%;
}
.menuplus ul li a {
 text-decoration:none;
 font-size: 20px;
 color: #fff5ee;
 text-transform: uppercase;
 
}
/*Barra de Busca*/

.busca {
 width: 40%;
 border: 1px solid #ccc;
 border-radius: 15%;
 margin-left: 5%;
 outline: none;
 height: 15px;
 padding: 15px;
 position: absolute;
 top: 63px;
}
.lupa img {
 position: relative;
 width: 20px;
 height: 20px;
 margin-left: 148px;
 top: 9px;
}

/*Conteudo*/
.texto h2 {
 text-align: center;
 margin-top: 20px;
 margin-bottom: 10px;
 border-bottom: 1px solid;
 font-weight: bold;
}
.texto p {
 text-align: justify;
 text-indent: 20px;
 margin-bottom: 20px;
 margin-left: 6px;
 margin-right: 6px;
}

.figure img {
 display: inline-block;
 margin-right: 5px;
 margin-left: 5px;
}
/*RodaPe*/
.rodape {
 background-color: #2f4f4f;
 border-top: 1px solid;
}
.rodape h3 {
 color:#fff;
 text-align: center;
 
}

